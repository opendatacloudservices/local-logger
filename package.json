    "@types/node": "^14.14.20",
    "@types/pg": "^7.14.7",
    "@types/uuid": "^8.3.0",
    "eslint": "^6.8.0",
    "gts": "^3.0.3",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.3",
    "@types/express": "^4.17.11"
